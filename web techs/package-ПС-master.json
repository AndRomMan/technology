  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-images"
    ],
    "syntax": "scss",
    "rules": {
      "images/broken": true,
      "indentation": 4,
      "declaration-empty-line-before": "never",
      "no-invalid-double-slash-comments": null,
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-after-same-name-blockless",
            "first-nested"
          ],
          "ignore": [
            "after-comment"
          ],
          "ignoreAtRules": [
            "mixin",
            "import",
            "extend"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "/^mixin/",
            "mixin",
            "define-mixin",
            "extend",
            "each",
            "for"
          ]
        }
      ]
    }
  }
